{
  "name": "ng-idle",
  "description": "A module for responding to idle users in Angular applications.",
  "version": "11.1.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rogerbassons/ng2-idle.git"
  },
  "author": "Ng-Idle Team",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/rogerbassons/ng2-idle.git/issues"
  },
  "homepage": "https://github.com/rogerbassons/ng2-idle.git#readme",
  "private": true,
  "scripts": {
    "clean:coverage": "rimraf coverage",
    "clean:dist": "rimraf dist",
    "ng": "ng",
    "start": "ng serve",
    "prebuild:libs": "npm run clean:dist",
    "build:libs": "ng build idle-core && ng build idle-keepalive",
    "prebuild:docs": "npm run build:libs",
    "build:docs": "cd ./projects/docs && npm link ../../dist/idle-core && npm link ../../dist/idle-keepalive && ng build docs --base-href=/ng2-idle/",
    "pretest": "npm run build:libs",
    "test": "ng test idle-core --watch=false --codeCoverage=true --browsers=ChromeHeadlessNoSandbox --progress=false && ng test idle-keepalive --watch=false --codeCoverage=true --browsers=ChromeHeadlessNoSandbox --progress=false",
    "precoverage": "npm run clean:coverage && npm test",
    "coverage": "cat coverage/idle-core/lcov.info coverage/keepalive/lcov.info | node_modules/.bin/coveralls",
    "lint": "ng lint idle-core && ng lint idle-keepalive",
    "lint:fix": "ng lint idle-core --fix=true && ng lint idle-keepalive --fix=true",
    "e2e": "ng e2e",
    "copy:info": "for d in ./dist/*; do cp LICENSE README.md $d; done",
    "release": "lerna version --conventional-commits",
    "postrelease": "npm publish dist/idle-core && npm publish dist/idle-keepalive",
    "version": "npm run build && npm run copy:info && node scripts/postrelease.js && git add ./package.json"
  },
  "dependencies": {
    "@angular/animations": "~13.3.8",
    "@angular/cdk": "^13.3.7",
    "@angular/common": "~13.3.8",
    "@angular/compiler": "~13.3.8",
    "@angular/core": "~13.3.8",
    "@angular/forms": "~13.3.8",
    "@angular/material": "^13.3.7",
    "@angular/platform-browser": "~13.3.8",
    "@angular/platform-browser-dynamic": "~13.3.8",
    "@angular/router": "~13.3.8",
    "@fortawesome/angular-fontawesome": "^0.10.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-brands-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "karma-coverage": "^2.2.0",
    "ngx-highlightjs": "^5.0.0",
    "rxjs": "~6.6.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.5",
    "@angular-eslint/builder": "13.2.1",
    "@angular-eslint/eslint-plugin": "13.2.1",
    "@angular-eslint/eslint-plugin-template": "13.2.1",
    "@angular-eslint/schematics": "13.2.1",
    "@angular-eslint/template-parser": "13.2.1",
    "@angular/cli": "~13.3.5",
    "@angular/compiler-cli": "~13.3.8",
    "@angular/language-service": "~13.3.8",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^14.14.30",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
    "codelyzer": "^6.0.0",
    "commitlint": "^12.1.4",
    "coveralls": "^3.1.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^8.12.0",
    "jasmine-core": "^4.1.1",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "lerna": "^4.0.0",
    "ng-packagr": "^13.3.1",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.6.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
